# API Keys (需要用户自己配置)
EDGE_API_KEY=your_edge_api_key_here
CLOUD_API_KEY=your_cloud_api_key_here

# Privacy Configuration
PRIVACY_BUDGET_LIMIT=10.0
AUDIT_LOG_LEVEL=INFO
COMPLIANCE_MODE=strict

# Model Configuration
EDGE_MODEL_PATH=./models/llama-3-8b-instruct
CLOUD_MODEL_NAME=gpt-4o-mini

# Privacy Protection Parameters
DIFFERENTIAL_PRIVACY_EPSILON=1.2
DIFFERENTIAL_PRIVACY_DELTA=0.00001
SALIENCY_MASKING_THRESHOLD=0.3
DIMENSIONALITY_REDUCTION_TARGET=64

# Performance Configuration
MAX_BATCH_SIZE=32
MAX_SEQUENCE_LENGTH=512
DEVICE=cpu

# Logging Configuration
LOG_LEVEL=INFO
ENABLE_AUDIT_LOGGING=true
ENABLE_PERFORMANCE_LOGGING=true
